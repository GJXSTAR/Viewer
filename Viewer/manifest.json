{
    "name": "Viewer",
    "version": "0.1",
    "description": "本地图像查看",
    "permissions": ["tabs"],
    "offline_enabled": true,
    "browser_action": {
        "default_title": "Viewer",
        "default_icon": {
            "16": "img/icon_16.png",
            "32": "img/icon_32.png",
            "48": "img/icon_48.png"
        }
    },
    "background": {
        "scripts": [ "js/background.js" ]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons": {
        "16": "img/icon_16.png",
        "32": "img/icon_32.png",
        "48": "img/icon_48.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
}